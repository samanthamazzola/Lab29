
@{
    ViewBag.Title = "MovieVault";
}

<br />
<div>
    <table method="post" class="table" value="Movie Vault" border="1">
        <tr>
            <td><label>Title</label></td>
            <td><label>Category</label></td>
            <td><label>Description</label></td>
        </tr>
        @foreach (var movie in ViewBag.movie)
        {
            <tr>
                <td>@movie.title</td>
                <td>@movie.category</td>
                <td>@movie.description</td>
            </tr>
        }
    </table>
</div>
<br />
<p><a class="btn btn-default" href="/Home/Index">Home Page &raquo;</a></p>


